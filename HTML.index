    <script>
        $(function () {
            var cookieVideo = readCookie("popupAlreadyShown");
            if (cookieVideo == null || cookieVideo == "" || cookieVideo == "false") {
                $('#staticBackdrop').modal('show')
            }
        });

        function FecharModal() {
            var cookieVideo = readCookie("popupAlreadyShown");

            if (cookieVideo == null || cookieVideo == "") {
                alert("Clique no video para poder fechar esse Popup");
            }
            else if (cookieVideo == "false") {
                alert("Aguarde até a liberação do Popup");
            }
            else {
                $('#staticBackdrop').modal('hide');
                $('#staticBackdrop').remove();
            }
        }

        function clickVideoPlay() {
            createCookie("popupAlreadyShown", "false", 0)
            $('.timeClose').removeClass("d-none").find("span").html("em <span class='tempoVideo'>" + ($('.timeClose').data("time") * 1) + "</span> segundos");

            setInterval(function () {
                var timeAtual = $('.tempoVideo').text() * 1;
                if (timeAtual > 0) {
                    $('.tempoVideo').text((timeAtual - 1));
                }
                else {
                    $('.timeClose').removeAttr("disabled").find("span").text("");
                    createCookie("popupAlreadyShown", "true", 365);
                }
            }, 1000);

        }

        function labnolIframe(div) {
            var iframe = document.createElement("iframe");
            iframe.setAttribute(
                "src",
                "https://www.youtube.com/embed/" + div.dataset.id + "?autoplay=1&rel=0"
            );
            iframe.setAttribute("frameborder", "0");
            iframe.setAttribute("allowfullscreen", "1");
            iframe.setAttribute(
                "allow",
                "accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
            );
            div.parentNode.replaceChild(iframe, div);
        }

        function initYouTubeVideos() {
            var playerElements = document.getElementsByClassName("youtube-player");
            for (var n = 0; n < playerElements.length; n++) {
                var videoId = playerElements[n].dataset.id;
                var div = document.createElement("div");
                div.setAttribute("data-id", videoId);
                var thumbNode = document.createElement("img");
                thumbNode.src = "https://sib1.app.br/imagens/PlayVideo.png".replace(
                    "ID",
                    videoId
                );
                div.appendChild(thumbNode);
                var playButton = document.createElement("div");
                playButton.setAttribute("class", "play");
                div.appendChild(playButton);
                div.onclick = function () {
                    labnolIframe(this);
                };
                playerElements[n].appendChild(div);
            }
        }

        document.addEventListener("DOMContentLoaded", initYouTubeVideos);
    </script>

