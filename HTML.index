<script type="text/javascript">
    var elementEdicaoFormularioSite;
    var timeoutEditFormulario;

    $(document).ready(function () {
        criarEdicaoFormularios();
    });
    if (window.Sys && Sys.WebForms && Sys.WebForms.PageRequestManager) {
        Sys.WebForms.PageRequestManager.getInstance().add_endRequest(function () {
            criarEdicaoFormularios();
        });
    }
    function criarEdicaoFormularios() {
        $("#dlgEdicaoFormulario").appendTo($("form:first"));
        try { criarEdicaoTopoFormulario(); } catch (ex) { console.log(ex); }
        try { criarEdicaoCamposFormulario(); } catch (ex) { console.log(ex); }
        try { criarSortCamposFormulario(); } catch (ex) { console.log(ex); }
        try { exibirLegendaEdicao(); } catch (ex) { console.log(ex); }
        try { acertarLabelsFormulario(); } catch (ex) { console.log(ex); }
    }
    function criarEdicaoTopoFormulario() {
        CKEDITOR.disableAutoInline = true;
        $('.topo_conteudo_formulario').each(function () {
            if (!$(this).hasClass('cke_editable') && $(this).next('.container_formulario').length > 0) {
                try {
                    var cid = $(this).next('.container_formulario').find('.cid').val();
                    $(this).attr('contenteditable', true);
                    var editor = CKEDITOR.inline(this);
                    editor.on('change', function (evt) {
                        saveConteudoTopoFormulario(editor, cid);
                    });
                } catch (ex) {
                    console.log(ex);
                }
            }
        });
    }
    function criarEdicaoCamposFormulario() {
        $('.container_formulario').each(function () {
            if ($(this).find('.edicao_formulario').length < 1) {
                var cef = $('<div>', {
                    class: 'edicao_formulario',
                    title: 'Editar os campos deste formulário',
                    'onclick': 'editarCamposFormulario(this)'
                });
                $('<i>', { class: 'fa fa-edit me-1' }).appendTo(cef);
                $('<span>', { text: 'Editar campos' }).appendTo(cef);
                $(this).prepend(cef);
            }
        });
    }
    function saveConteudoTopoFormulario(editor, request) {
        if (timeoutEditFormulario)
            clearTimeout(timeoutEditFormulario);
        timeoutEditFormulario = setTimeout(function () {
            saveEditControl(request, editor.name);
        }, 3000);
    }
    function editarCamposFormulario(element) {
        elementEdicaoFormularioSite = $(element);
        var idFormulario = $(element).parent().find('.id_formulario').val();
        $('#hfIdFormularioEdicao').val(idFormulario);
        $('.loading_edicao_formulario').css('display', 'flex');
        $('.container_edicao_formulario').hide();
        $('#dlgEdicaoFormulario').modal('show');
        $('#btnCarregarCamposFormulario').click();
    }
    function criarSortCamposFormulario() {
        $(".lista_campos_formulario").sortable({
            tolerance: "pointer",
            items: ".item_campo_formulario",
            handle: ".pcf_order",
            stop: function (evt) {
                var indexes = '';
                $('.pcf_order').each(function (index) {
                    indexes += $(this).data('id') + ';';
                });
                $('#ctl00_ControlEdicao_ControlEdicaoFormulario_hfPrioridadeCamposFormulario').val(indexes);
                $('#btnSalvarOpcoesEdicaoFormulario').click();
            },
        }).disableSelection();
    }
    function toggleEdicaoCampoTexto(element) {
        var habilitarEdicao = $(element).parent().find('.form-control').is(':disabled');
        if (habilitarEdicao) {
            $(element).parent().find('.form-control').removeAttr('disabled');
            $(element).parent().find('.not_edit_formulario').hide();
            $(element).parent().find('.edit_formulario').show();
        } else {
            $(element).parent().find('.form-control').attr('disabled', 'disabled');
            $(element).parent().find('.edit_formulario').hide();
            $(element).parent().find('.not_edit_formulario').show();
        }
        exibirLegendaEdicao();
    }
    function toggleObrigatoriedadeCampo(element) {
        var obrigatorio = $(element).find('i').hasClass('fa-check-circle');
        if (obrigatorio) {
            $(element).find('i').removeClass().addClass('fa fa-times-circle text-danger');
        } else {
            $(element).find('i').removeClass().addClass('fa fa-check-circle text-success');
        }
    }
    function exibirLegendaEdicao() {
        var edicaoAtiva = $('.edit_formulario:visible').length > 0;
        if (edicaoAtiva)
            $('.legenda_edicao_campos').show('toggle');
        else
            $('.legenda_edicao_campos').hide('toggle');
    }
    function excluirCampoFormulario(element) {
        if (!confirm('Deseja realmente excluir este campo do formulário?')) return false;
        $(element).attr('disabled', 'disabled');
        $(element).find('i').removeClass().addClass('fa fa-asterisk fa-spin');
        $(element).closest('.item_campo_formulario').hide('slide', function () { $(this).remove(); });
    }
    function carregarFormularioEditado(element) {
        $('#dlgEdicaoFormulario').modal('hide');
        $(elementEdicaoFormularioSite).closest('.container_formulario').find('.atualizar_formulario').click();
    }
</script>

        
