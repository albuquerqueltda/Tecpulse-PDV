        Sys.WebForms.PageRequestManager.getInstance().add_endRequest(function () {
            adicionarEdicaoParallaxEdicaoSite();
        });
    }
    function adicionarEdicaoParallaxEdicaoSite() {
        $('section[data-type="0"]').each(function () {
            if ($(this).find('.div_edicao_parallax').length < 1) {
                var divEdicao = $('<div>', {
                    class: 'div_edicao_parallax btn btn-warning btn-sm',
                    title: 'Editar esta imagem',
                    text: 'Editar imagem',
                    onclick: 'editarParallaxIA(this)'
                }).appendTo($(this));
            }
        });
    }
    function editarParallaxIA(element) {
        var section = $(element).closest('section');
        var cid = $(section).data('cid');
        var params = $(section).find('.parallax').data('parallax-image');
        var atributes = getIAAtributesByParams(params.substring(params.indexOf('?')));
        var queryAtual = $.trim(atributes.query).replace('JPG', '').replace('PNG', '');
        parallaxToChange = $(section);
        $('#tbxPromptImagemEdicaoParallax').val(queryAtual);
        $('#tbxPromptImagemEdicaoParallax').data('idia', atributes.idia);
        $('#tbxPromptImagemEdicaoParallax').data('orientation', atributes.orientation);
        $('#tbxPromptImagemEdicaoParallax').data('url', params);
        $('#tbxPromptImagemEdicaoParallax').data('cid', cid);
        $('.container_imagens_parallax_edicao_site').hide();
        $('.loading_parallax').show();
        $('#dlgEdicaoParallax').modal('show');
        buscarImagensSugeridasParallax();
    }
    function buscarImagensSugeridasParallax() {
        $('.btn_buscar_parallax').text('Aguarde');
        $('.btn_buscar_parallax').attr('disabled', 'disabled');
        var queryImagem = $('#tbxPromptImagemEdicaoParallax').val();
        var idia = $('#tbxPromptImagemEdicaoParallax').data("idia");
        var urlImagem = $('#tbxPromptImagemEdicaoParallax').data("url");
        var orientation = $('#tbxPromptImagemEdicaoParallax').data("orientation");
        var cid = $('#tbxPromptImagemEdicaoParallax').data("cid");

        $.ajax({
            type: 'GET',
            url: '../EditControls.ashx',
            async: true,
            processdata: true,
            crossDomain: true,
            data: {
                get_images: true,
                requestConfig: cid,
                query: queryImagem,
                iaID: idia,
                orientacao: orientation
            },
            success: function (data, textStatus, jqXHR) {
                carregarImagensParallax(idia, cid, data);
            },
            error: function (jqXHR, textStatus, errorThrown) {
                toastDanger('', errorThrown.trim());
            },
            complete: function (jqXHR, textStatus) {
                $('.btn_buscar_parallax').removeAttr('disabled');
                $('.btn_buscar_parallax').text('Buscar');
                $('.container_imagens_parallax_edicao_site').show();
                $('.loading_parallax').hide();
            }
        });
    }
    function getIAAtributesByParams(parametros) {
        var urlSearchParams = new URLSearchParams(parametros);
        //Query utilizada
        var queryAux = urlSearchParams.get('qry');
        //ID da imagem
        var idIMGIA = urlSearchParams.get('iaID');
        var orientacaoAux = urlSearchParams.get('oIA');
        //Orientação da imagem
        var orientacaoIA = getOrientacaoByName(orientacaoAux);
        return { query: queryAux, idia: idIMGIA, orientation: orientacaoIA };
    }
    function carregarImagensParallax(idSelecionada, cid, array) {
        var container = $('.container_edicao_parallax');
        container.html('');
        console.log(idSelecionada);
        var obj = jQuery.parseJSON(array);
        $.each(obj, function (key, value) {
            var divImage = $('<div>', {
                class: 'img_conteudo_edicao col-md-4 col-6' + ((value.id == idSelecionada) ? ' active' : ''),
                'data-id': value.id,
                'data-title': value.title,
                'data-used': value.used,
                title: 'Substituir a imagem atual por esta',
                'onclick': "substituirImagemParallax(this,'" + cid + "','" + value.urlImagem + "')"
            });
            var img = $('<img>', {
                alt: value.title,
                src: value.urlImagem
            }).appendTo(divImage);
            $(divImage).appendTo(container);
        });
    }
    function substituirImagemParallax(element, cid, src) {
        $.ajax({
            type: 'POST',
            url: '../EditControls.ashx',
            async: true,
            processdata: true,
            crossDomain: true,
            data: {
                save_imagem_parallax: true,
                request: cid,
                objeto: src
            },
            success: function (data) {
                var containerParallax = $(parallaxToChange).find('.parallax');
                $(containerParallax).data('parallax-image', src);
                $(containerParallax).css('background-image', 'url(' + src + ')');

                containerParallax = $(parallaxToChange).find('.img_parallax');
                $(containerParallax).data('img', src);
                $(containerParallax).css('background-image', 'url(' + src + ')');

                $('#dlgEdicaoParallax').modal('hide');
                toastSuccess('', 'Conteúdo salvo');
            },
            error: function (data) {
                toastDanger('', data.statusText);
            },
            complete: function (data) {
            }
        });
    }
    function keyPressSearchParallax(event) {
        var code = (event.keyCode ? event.keyCode : event.which);
        if (code == 13) {
            buscarImagensSugeridasParallax();
        }
    }
</script>

        
