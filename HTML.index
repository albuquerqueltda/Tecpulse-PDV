<script type="text/javascript">
    var timeoutEdit;
    var imgToChange;

    criarEdicaoConteudosInformativos();
    if (window.Sys && Sys.WebForms && Sys.WebForms.PageRequestManager) {
        Sys.WebForms.PageRequestManager.getInstance().add_endRequest(function () {
            criarEdicaoConteudosInformativos();
        });
    }

    function criarEdicaoConteudosInformativos() {
        CKEDITOR.disableAutoInline = true;
        criarEdicaoImagensConteudoInformativo();
        CKEDITOR.on('instanceReady', function (evt) {
            criarEdicaoImagensConteudoInformativo();
        });
        $('.container_conteudo_informativo').each(function () {
            if (!$(this).hasClass('cke_editable')) {
                try {
                    var cid = $(this).data('cid');
                    $(this).attr('contenteditable', true);
                    var editor = CKEDITOR.inline(this);
                    editor.on('change', function (evt) {
                        saveConteudoInformativo(editor, cid);
                    });
                } catch (ex) {
                    console.log(ex);
                }
            }
        });
    }
    function criarEdicaoImagensConteudoInformativo() {
        $('.container_conteudo_informativo img').unbind("click").click(function (event) {
            if (($(this).attr('src') != undefined) && ($(this).attr('src').indexOf('iaID') > -1)) {
                editarImagemIA(event, $(this));
            }
        });
    }
    function saveConteudoInformativo(editor, request) {
        try {
            if (timeoutEdit)
                clearTimeout(timeoutEdit);
            timeoutEdit = setTimeout(function () {
                saveEditControl(request, editor.name);
            }, 3000);
        } finally {
            criarEdicaoImagensConteudoInformativo();
        }
    }
    function saveEditControl(requestAux, idEditor) {
        $.ajax({
            type: 'POST',
            url: '../EditControls.ashx',
            async: true,
            processdata: true,
            crossDomain: true,
            data: {
                save_edit_control: true,
                request: requestAux,
                objeto: RemoveClassCKeditor(idEditor)
            },
            success: function (data) {
                toastSuccess('', 'Conteúdo salvo');
            },
            error: function (data) {
                toastDanger('', data.statusText);
            },
            complete: function (data) {
            }
        });
    }
    function RemoveClassCKeditor(conteudo) {
        var htmlCk = CKEDITOR.instances[conteudo];
        if (htmlCk != null) {
            return htmlCk.getData();
        }
        else {
            return conteudo;
        }
    }
    function getOrientacaoByName(oriAux) {
        try {
            if (oriAux.indexOf('portrait') > -1)
                oriAux = 'portrait';
            else if (oriAux.indexOf('landscape') > -1)
                oriAux = 'landscape';
            else if (oriAux.indexOf('square') > -1)
                oriAux = 'square';
            else oriAux = 'panoramic';
            return oriAux;
        } catch (ex) {
            console.log(ex);
            return 'portrait';
        }
    }
    function editarImagemIA(event, element) {
        imgToChange = $(element);
        $(element).closest('.container_conteudo_informativo').data('editActive', 'true');
        $(element).closest('.container_conteudo_publicacao').data('editActive', 'true');
        $('.container_imagens_edicao').hide();
        $('.loading_imagens').show();
        $('#dlgEdicaoImagemConteudo').modal('show');

        var atributes = getIAAtributesBySrc(element);
        var queryImg = $('#hfQueryImagemSelecionadaConteudoInformativo').val().replace("?", "?&");

        $('#ctl00_ControlEdicao_ControlEdicaoConteudoInformativo_tbxPromptImagem').val($.trim(atributes.query)).attr({
            "data-idia": atributes.idia,
            "data-orientation": atributes.orientation,
            "data-url": queryImg,
            "data-ci": $('#hfCIEdicaoImagensConteudoInformativo').val()
        });
        //Buscando as imagens relacionadas
        BuscarImagemIA();
        //Carregando os atributos necessários para edições
        var srcAux = $(imgToChange).attr('src');
        var ci = $(imgToChange).closest('.container_conteudo_publicacao').data('cid');
        if (!ci || ci == null || ci == undefined)
            ci = $(imgToChange).closest('.container_conteudo_informativo').data('cid');

        $('#hfCIEdicaoImagensConteudoInformativo').val(ci);
        $('#hfQueryImagemSelecionadaConteudoInformativo').val(srcAux);
        $('#btnCarregarImagensConteudoEdicao').click();
        event.preventDefault();
        event.stopPropagation();
        return false;
    }
    function BuscarImagemIA() {
        $('.btn_buscar_imagens_edicao_conteudo').attr('disabled', 'disabled');
        $('.btn_buscar_imagens_edicao_conteudo').text('Aguarde');

        var queryImagem = $('#ctl00_ControlEdicao_ControlEdicaoConteudoInformativo_tbxPromptImagem').val();
        var idia = $('#ctl00_ControlEdicao_ControlEdicaoConteudoInformativo_tbxPromptImagem').data("idia");
        var urlImagem = $('#ctl00_ControlEdicao_ControlEdicaoConteudoInformativo_tbxPromptImagem').data("url");
        var orientation = $('#ctl00_ControlEdicao_ControlEdicaoConteudoInformativo_tbxPromptImagem').data("orientation");
        var ci = $('#ctl00_ControlEdicao_ControlEdicaoConteudoInformativo_tbxPromptImagem').data("ci");

        var requestConfig = $('#requestIA').val();

        $.ajax({
            type: 'GET',
            url: '../EditControls.ashx',
            async: true,
            processdata: true,
            crossDomain: true,
            data: {
                get_images: true,
                requestConfig: requestConfig,
                query: queryImagem,
                iaID: idia,
                orientacao: orientation
            },
            success: function (data, textStatus, jqXHR) {
                carregarImagensConteudo(idia, data);
            },
            error: function (jqXHR, textStatus, errorThrown) {
                toastDanger('', errorThrown);
            },
            complete: function (jqXHR, textStatus) {
                $('.btn_buscar_imagens_edicao_conteudo').removeAttr('disabled');
                $('.btn_buscar_imagens_edicao_conteudo').text('Buscar');
                $('.container_imagens_edicao').show();
                $('.loading_imagens').hide();
            }
        });
        if (event) {
            event.preventDefault();
            event.stopPropagation();
        }
        return false;
    }
    function carregarImagensConteudo(idSelecionada, array) {
        if ($('.container_imagens_edicao .container_imagens_sugeridas').length < 1)
            return;
        var container = $('.container_imagens_edicao .container_imagens_sugeridas');
        container.html('');
        var obj = jQuery.parseJSON(array);
        $.each(obj, function (key, value) {
            var divImage = $('<div>', {
                class: 'img_conteudo_edicao col-md-4 col-6' + ((value.id == idSelecionada) ? ' active' : ''),
                'data-id': value.id,
                'data-title': value.title,
                'data-used': value.used,
                title: 'Substituir a imagem atual por esta',
                'onclick': "substituirImagemIA(this,'" + value.urlImagem + "')"
            });
            var img = $('<img>', {
                alt: value.title,
                src: value.urlImagem
            }).appendTo(divImage);
            $(divImage).appendTo(container);
        });
    }
    function substituirImagemIA(element, url) {
        $('.img_conteudo_edicao').removeClass('active');
        $(element).addClass('active');
        $(imgToChange).attr('src', url);
        $(imgToChange).attr('data-cke-saved-src', url);

        var ci = $(imgToChange).closest('.container_conteudo_publicacao');
        if (!ci || ci == null || ci == undefined || ci.length < 1)
            ci = $(imgToChange).closest('.container_conteudo_informativo');

        if (ci && ci != undefined) {
            var req = $(ci).data('cid');
            saveEditControl(req, $(ci).attr("id"));
        }
        criarEdicaoImagensConteudoInformativo();
        $('#dlgEdicaoImagemConteudo').modal('hide');
    }
    function getIAAtributesBySrc(element) {
        var urlImagem = $(element).attr('src');
        var urlSearchParams = new URLSearchParams(urlImagem.substring(urlImagem.indexOf('?')));

        //Query utilizada
        var queryAux = urlSearchParams.get('qry');

        //ID da imagem
        var idIMGIA = urlSearchParams.get('iaID');

        //Orientação da imagem
        var orientacaoIA = getOrientacaoByName(urlSearchParams.get('oIA'));

        return { query: queryAux, idia: idIMGIA, orientation: orientacaoIA };
    }
    function uploadImagemEdicaoConteudoInformativo(element) {
        if ($('#fupUploadImagemConteudoInformativo')[0].files.length < 1) {
            toastWarning('Atenção', 'Nenhuma imagem selecionada!');
            return;
        }
        var ci = $(imgToChange).closest('.container_conteudo_publicacao').data('cid');
        if (!ci || ci == null || ci == undefined)
            ci = $(imgToChange).closest('.container_conteudo_informativo').data('cid');
        var htmlAntigo = $(element).html();
        $(element).attr('disabled', 'disabled').text('Aguarde o upload...');
        var formData = new FormData();
        var file = $('#fupUploadImagemConteudoInformativo')[0].files[0];
        var ajaxUrl = "../EditControls.ashx";

        formData.append('upload_imagem_conteudo', true);
        formData.append('file', file);
        formData.append('request', ci);
        $.ajax({
            url: ajaxUrl,
            type: "POST",
            data: formData,
            contentType: false,
            processData: false
        }).done(function (response) {
            $('#fupUploadImagemConteudoInformativo').val('');
        }).fail(function (response) {
            toastDanger('Ocorreu um erro', response.responseText);
        }).always(function () {
            $(element).html(htmlAntigo).removeAttr('disabled');
            $('#btnCarregarImagensConteudoEdicao').click();
        });
    }
    function keyPressSearchImagemConteudo(event) {
        var code = (event.keyCode ? event.keyCode : event.which);
        if (code == 13) {
            BuscarImagemIA();
        }
    }
</script>

        

