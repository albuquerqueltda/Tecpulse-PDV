    function criarEdicaoConteudosPublicacao() {
        //Título das publicações
        CKEDITOR.disableAutoInline = true;
        $('.title-page .control_publicacao_titulo').each(function () {
            if (!$(this).hasClass('cke_editable')) {
                try {
                    var cid = $(this).data('cid');
                    $(this).attr('contenteditable', true);
                    var editor = CKEDITOR.inline(this);
                    editor.on('change', function (evt) {
                        savePublicacaoJS(editor, cid, 0);
                    });
                } catch (ex) {
                    console.log(ex);
                }
            }
        });
        //Resumo
        $('.title-page .control_publicacao_resumo').each(function () {
            if (!$(this).hasClass('cke_editable')) {
                try {
                    var cid = $(this).data('cid');
                    $(this).attr('contenteditable', true);
                    var editor = CKEDITOR.inline(this);
                    editor.on('change', function (evt) {
                        savePublicacaoJS(editor, cid, 1);
                    });
                } catch (ex) {
                    console.log(ex);
                }
            }
        });
        //Conteúdo
        $('.control_publicacao_conteudo').each(function () {
            if (!$(this).hasClass('cke_editable')) {
                try {
                    var cid = $(this).data('cid');
                    $(this).attr('contenteditable', true);
                    var editor = CKEDITOR.inline(this);
                    editor.on('change', function (evt) {
                        savePublicacaoJS(editor, cid, 2);
                    });
                } catch (ex) {
                    console.log(ex);
                }
            }
        });
        console.log('imagens edit');
        //Imagens
        $('.control_publicacao_conteudo img').unbind("click").click(function (event) {
            if (($(this).attr('src') != undefined) && ($(this).attr('src').indexOf('iaID') > -1)) {
                editarImagemIA(event, $(this));
            }
        });
    }

    function savePublicacaoJS(editor, request, tipo) {
        try {
            if (timeoutEdit)
                clearTimeout(timeoutEdit);
            timeoutEdit = setTimeout(function () {
                saveControlPublicacaoEdicaoSite(request, editor.name, tipo);
            }, 3000);
        } catch (ex) {
            console.log(ex);
        }
    }
    function saveControlPublicacaoEdicaoSite(requestAux, nomeCKEditor, tipo) {
        $.ajax({
            type: 'POST',
            url: '../EditControls.ashx',
            async: true,
            processdata: true,
            crossDomain: true,
            data: {
                save_edit_control: true,
                request: requestAux,
                objeto: RemoveClassCKeditor(nomeCKEditor),
                tipo: tipo
            },
            success: function (data) {
                toastSuccess('', 'Conteúdo salvo');
            },
            error: function (data) {
                toastDanger('', data.statusText);
            },
            complete: function (data) {
            }
        });
    }
    function criarAdicaoFotosPublicacaoEdicaoSite() {
        if ($('.btn_adicionar_foto_publicacao').length > 0)
            return;
        var divBotao = $('<div>', {
            class: 'text-center btn_adicionar_foto_publicacao my-2'
        });
        var botao = $('<div>', {
            class: 'btn btn-outline-primary btn-sm',
            'onclick': '$(".container_listagem_fotos_publicacao input[type=file]").click()'
        }).appendTo(divBotao);
        var iBotao = $('<i>', {
            class: 'fa fa-plus-circle me-1'
        }).appendTo(botao);
        var spanBotao = $('<span>', {
            text: 'Adicionar novas fotos'
        }).appendTo(botao);
        var inputUpload = $('<input>', {
            type: 'file',
            class: 'd-none',
            multiple: true,
            accept: 'image/png, image/gif, image/jpeg, image/jpg, image/webp',
            onchange: 'uploadFilesEdicaoPublicacaoEdicaoSite(this)'
        });
        $('.container_listagem_fotos_publicacao').prepend(inputUpload);
        $('.container_listagem_fotos_publicacao').prepend(divBotao);
    }
    function uploadFilesEdicaoPublicacaoEdicaoSite(element) {
        if ($(element)[0].files.length < 1) {
            toastWarning('', 'Nenhum arquivo selecionado!');
            return;
        }
        $('.btn_adicionar_foto_publicacao').find('i').removeClass().addClass('fa fa-asterisk fa-spin me-1');
        $('.btn_adicionar_foto_publicacao').find('span').text('Aguarde o upload...');
        $('.btn_adicionar_foto_publicacao').unbind();
        $('.btn_adicionar_foto_publicacao').off('click');
        var formData = new FormData();
        var ajaxUrl = "../EditControls.ashx";
        var requestAux = $('#requestIA').val();
        var idPublicacao = $(element).closest('.container_listagem_fotos_publicacao').parent().data('id');

        formData.append('upload_imagem_publicacao', true);
        formData.append('request', requestAux);
        formData.append('idPublicacao', idPublicacao);
        for (index = 0; index < $(element)[0].files.length; index++) {
            formData.append('file_' + index, $(element)[0].files[index]);
        }
        $.ajax({
            url: ajaxUrl,
            type: "POST",
            data: formData,
            contentType: false,
            processData: false
        }).done(function (response) {
            $(element).val(null);
        }).fail(function (response) {
            console.log(response);
            toastDanger('Ocorreu um erro', response.responseText);
        }).always(function () {
            atualizarFotosEdicaoPublicacaoEdicaoSite();
        });
    }
    function atualizarFotosEdicaoPublicacaoEdicaoSite() {
        $('#btnAtualizarFotosEdicaoPublicacaoEdicaoSite').click();
        $('.btn_adicionar_foto_publicacao').find('i').removeClass().addClass('fa fa-plus-circle me-1');
        $('.btn_adicionar_foto_publicacao').find('span').text('Adicionar novas fotos');
        $('.btn_adicionar_foto_publicacao').off('click');
        $('.btn_adicionar_foto_publicacao').click(function () {
            $(".container_listagem_fotos_publicacao input[type=file]").click();
        });
    }
    function criarOpcoesEdicaoImagemPublicacaoEdicaoSite() {
        $('.box-image-galeria .cboxElement').each(function () {
            if ($(this).find('.opcoes_edicao_foto').length < 1) {
                var divOpcoes = $('<div>', {
                    class: 'opcoes_edicao_foto'
                }).appendTo($(this));
                //Sort
                var opcaoSort = $('<span>', {
                    class: 'btn btn-light sort_fep me-2',
                    title: 'Reordenar imagens',
                }).appendTo(divOpcoes);
                var iOpcaoSort = $('<i>', {
                    class: 'fa fa-arrows'
                }).appendTo(opcaoSort);
                //Exclusão
                var opcaoExclusao = $('<span>', {
                    class: 'btn btn-danger',
                    title: 'Excluir esta imagem',
                    onclick: 'excluirFotoEdicaoPublicacaoEdicaoSite(this)'
