        $('.btnZap').prepend(editContainerWhatsapp);
        aplicarMarcaraNumeroWhatsapp();
    }
    function aplicarMarcaraNumeroWhatsapp(){
       try{
            var maskBehaviorEdicaoWpp = function (val) { return val.replace(/\D/g, '').length === 11 ? '(00) 00000-0000' : '(00) 0000-00009'; };
            var optionsMaskEdicaoWpp = { onKeyPress: function (val, e, field, options) { field.mask(maskBehaviorEdicaoWpp.apply({}, arguments), options); } };
            $('#ctl00_ControlEdicao_ControlEdicaoWhatsapp_tbxTelefone').mask(maskBehaviorEdicaoWpp, optionsMaskEdicaoWpp);
        }catch(ex){
            console.log(ex);
        }
    }
    function iniciarAlteracaoWhatsapp(event) {
        $('.loading_whatsapp').css('display', 'flex');
        $('.container_whatsapp').hide();
        $('#dlgEdicaoWhatsapp').modal('show');
        $('.btnZap').attr('onclick', '').unbind('click');
        $('#btnCarregarWhatsapp').click();
    }
    function updateBotaoWhatsapp(element) {
        var srcNovo = $(element).find('img').attr('src');
        $('.btnZap img').attr('src', srcNovo);
        $('.item_whatsapp').removeClass('active');
        $(element).addClass('active');
    }
    function toogleWhatsapp(element) {
        var possuiWhatsapp = $(element).find('i').hasClass('fa-toggle-on');
        if (possuiWhatsapp) {
            $(element).find('i').removeClass('fa-toggle-on').removeClass('text-primary').addClass('fa-toggle-off');
            $('.btnZap, .container_opcoes_whatsapp').hide();
        } else {
            $(element).find('i').removeClass('fa-toggle-off').addClass('fa-toggle-on text-primary');
            $('.btnZap, .container_opcoes_whatsapp').show();
        }
    }
    function togglePulsarWhatsapp(element) {
        var pulsarAtual = $(element).find('i').hasClass('fa-toggle-on');
        if (pulsarAtual) {
            $(element).find('i').removeClass('fa-toggle-on').removeClass('text-primary').addClass('fa-toggle-off');
            $('.btnZap section').removeClass();
        } else {
            $(element).find('i').removeClass('fa-toggle-off').addClass('fa-toggle-on text-primary');
            $('.btnZap section').addClass('tada infinite animated');
        }
    }
    function alterarPosicaoBotaoWhatsapp() {
        var posicaoVertical = $('#ddlPosicaoVerticalWhatsapp').val();
        var posicaoHorizontal = $('#ddlPosicaoHorizontalWhatsapp').val();
        var marginX = $('#txtHorizontalMarginWhatsapp').val();
        var marginY = $('#txtVerticalMarginWhatsapp').val();
        $('.btnZap').removeClass().addClass('btnZap').addClass(posicaoVertical).addClass(posicaoHorizontal);
        var styleAux = (posicaoHorizontal + ': ' + marginX + 'px !important;')
            + (posicaoVertical + ': ' + marginY + 'px !important;');
        $('.btnZap').attr('style', styleAux);
        $('#hfPosicaoHorizontalWhatsapp').val(marginX);
        $('#hfPosicaoVerticalWhatsapp').val(marginY);
        $('#btnSalvarPosicaoBotaoWhatsapp').click();
    }
</script>
