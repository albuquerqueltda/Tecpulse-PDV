<script type="text/javascript">
    var editors = [];
    var indexCodeMirrors = 0;
    configurarCamposEdicaoCssJS();
    criarSortSecoesEditaveis();
    removerEdicaoDosConteudosSecoesEditaveis();
    if (window.Sys && Sys.WebForms && Sys.WebForms.PageRequestManager) {
        Sys.WebForms.PageRequestManager.getInstance().add_endRequest(function () {
            configurarCamposEdicaoCssJS();
            criarSortSecoesEditaveis();
            removerEdicaoDosConteudosSecoesEditaveis();
        });
    }
    function configurarCamposEdicaoCssJS() {
        indexCodeMirrors = 0;
        $('.edicao_css_js').each(function (index) {
            if ($(this).parent().find('.CodeMirror').length < 1) {
                var modeAux = $(this).data('code') == '1' ? 'text/javascript' : 'text/css';
                var editor = CodeMirror.fromTextArea(this, {
                    mode: modeAux,
                    extraKeys: { "Ctrl-Space": "autocomplete" },
                    value: document.documentElement.innerHTML,
                    selectionPointer: true,
                    lineNumbers: true,
                    styleActiveLine: true,
                    matchBrackets: true,
                    lineWrapping: true
                });

                var charWidth = editor.defaultCharWidth(), basePadding = 4;
                editor.on("renderLine", function (cm, line, elt) {
                    var off = CodeMirror.countColumn(line.text, null, cm.getOption("tabSize")) * charWidth;
                    elt.style.textIndent = "-" + off + "px";
                    elt.style.paddingLeft = (basePadding + off) + "px";
                });
                editor.refresh();
                editors[indexCodeMirrors] = editor;
                indexCodeMirrors++;
            }
        });
    }
    function refreshEditors() {
        if (editors && editors != undefined)
            for (index = 0; index < editors.length; index++) {
                editors[index].refresh();
            }
    }
    function setEditors() {
        for (index = 0; index < editors.length; index++) {
            $(editors[index].getTextArea()).val(editors[index].getValue());
        }
    }
    function iniciarEdicaoGeralSite() {
        $('.loading_edicao_site_geral').css('display', 'flex');
        $('.container_edicao_site_geral').hide();
        $('#dlgEdicaoGeralSite').offcanvas('show');
    }
    function toggleOpcaoGeralSite(element) {
        var habilitado = $(element).find('i').hasClass('fa-toggle-on');
        if (habilitado)
            $(element).find('i').removeClass('fa-toggle-on').removeClass('text-primary').addClass('fa-toggle-off');
        else
            $(element).find('i').removeClass('fa-toggle-off').addClass('fa-toggle-on text-primary');
        atualizarOpcoesSelecionadasSite();
    }
    function atualizarOpcoesSelecionadasSite() {
        var botaoTopo = $('#btnAlterarBotaoTopoMenuSite').find('i').hasClass('fa-toggle-on');
        var formulario = $('#btnAlterarFormularioSite').find('i').hasClass('fa-toggle-on');
        var botaoWhatsapp = $('#btnAlterarBotaoWhatsAppSite').find('i').hasClass('fa-toggle-on');
        var botaoTraducoes = $('#btnAlterarTraducoesSite').find('i').hasClass('fa-toggle-on');
        var pluginLGPD = $('#btnAlterarLGPDSite').find('i').hasClass('fa-toggle-on');
        var jivoChat = $('#btnAlterarLGPDSite').find('i').hasClass('fa-toggle-on');


        if (botaoTopo) $('.btn_cta_topo').show(); else $('.btn_cta_topo').hide();
        if (botaoWhatsapp) $('#whats_flutuante').show(); else $('#whats_flutuante').hide();
        if (botaoTraducoes) $('.traducao.div_control').show(); else $('.traducao.div_control').hide();
        if (pluginLGPD) $('.banner-cookie').show(); else $('.banner-cookie').hide();
        if (formulario) $('.form-info').show(); else $('.form-info').hide();
        if (jivoChat) $('#jvlabelWrap').show(); else $('#jvlabelWrap').hide();
    }
    function toggleOpcoesEdicaoGeralSite(element) {
        $(element).closest('.card').find('.body-card').slideToggle({
            duration: 400,
            start: function () {
            },
            done: function () {
                $(element).find('.btn i').toggleClass('fa-minus-circle');
