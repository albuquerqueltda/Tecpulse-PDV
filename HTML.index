<div class="toast-container p-3 me-2"></div>

<script type="text/javascript">
    function alert(msg) {
        toastInfo(null, msg);
    }
    function toastInfo(title, msg) {
        toastAlert('info', title, msg, 5000);
    }
    function toastWarning(title, msg) {
        toastAlert('warning', title, msg, 5000);
    }
    function toastSuccess(title, msg) {
        toastAlert('success', title, msg, 5000);
    }
    function toastDanger(title, msg) {
        toastAlert('danger', title, msg, 5000);
    }
    function toastAlert(toastType, title, msg, timeout) {
        var toast = createTemplateMsg(toastType, title, msg);
        if (!timeout || timeout == undefined)
            timeout = 5000;
        toast.appendTo($('.toast-container'));
        toast.show('toggle');
        setTimeout(function () {
            hideToast(toast);
        }, timeout);
    }
    function hideToast(element) {
        try {
            $(element).closest('.toast').fadeOut('slow', function () {
                $(element).remove();
            });
        } catch (ex) { console.log(ex); }
    }
    function createTemplateMsg(toastType, title, msg) {
        if (!toastType || toastType == undefined || toastType == '')
            toastType = 'warning';
        var toast = $('<div>', {
            class: 'toast no-title ' + toastType,
            'role': 'alert',
            'aria-live': 'assertive',
            'aria-atomic': 'true'
        });
        //Header
        if (title && title != undefined && title != '') {
            header = $('<div>', {
                class: 'toast-header'
            });
            if (title && title != undefined && title != '') {
                title = 'Atenção';
            }
            $('<strong>', {
                class: 'me-auto',
                text: title
            }).appendTo(header);
            header.appendTo(toast);
            $(toast).removeClass('no-title');
        }
        $('<span>', {
            class: 'fa fa-times-circle close-toast',
            title: 'Fechar esta mensagem',
            'data-bs-dismiss': 'toast',
            'aria-label': 'Close',
            'style': 'cursor:pointer',
            'onclick': 'hideToast(this)'
        }).appendTo(toast);
        //body
        $('<div>', {
            class: 'toast-body',
            text: msg
        }).appendTo(toast);
        return toast;
    }
</script>

    
    
