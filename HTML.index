    function atualizarCorLayoutEdicaoSite(element) {
        try {
            var prev = $(element).prev();
            var next = $(element).next();
            var valorAtual = $(element).val();
            if (valorAtual.indexOf('rgb') > -1)
                valorAtual = rgb2hex(valorAtual);
            else if (valorAtual.length == 4) {
                valorAtual = "#" + valorAtual.charAt(1) + valorAtual.charAt(1)
                    + valorAtual.charAt(2) + valorAtual.charAt(2)
                    + valorAtual.charAt(3) + valorAtual.charAt(3);
            }
            if (prev && prev != undefined)
                prev.val(valorAtual);
            if (next && next != undefined)
                next.val(valorAtual);
        } catch (ex) {
            toastWarning('', 'Não reconhecemos a cor que foi informada!');
        } finally {
            saveValorDefinicaoAtributo(element);
