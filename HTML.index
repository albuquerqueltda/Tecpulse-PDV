            }
        });
        criarSortImagensEdicaoPublicacaoEdicaoSite();
    }
    function criarSortImagensEdicaoPublicacaoEdicaoSite() {
        $(".listagem_fotos_galeria_site").sortable({
            tolerance: "pointer",
            items: ".box-image-galeria",
