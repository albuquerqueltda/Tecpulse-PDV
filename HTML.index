<script type="text/javascript">
    criarEdicaoListagemPublicacoes();
    if (window.Sys && Sys.WebForms && Sys.WebForms.PageRequestManager) {
        Sys.WebForms.PageRequestManager.getInstance().add_endRequest(function () {
            criarEdicaoListagemPublicacoes();
        });
    }
    function criarEdicaoListagemPublicacoes() {
        //Título das publicações
        CKEDITOR.disableAutoInline = true;
        $('.control_listagem_publicacao_titulo').each(function () {
            if (!$(this).hasClass('cke_editable')) {
                try {
                    var cid = $(this).data('cid');
                    $(this).attr('contenteditable', true);
                    var editor = CKEDITOR.inline(this);
                    editor.on('change', function (evt) {
                        savePublicacaoJS(editor, cid);
                    });
                } catch (ex) {
                    console.log(ex);
                }
            }
        });
        //Criando o do conteúdo da publicação no popup
        try {
            CKEDITOR.replace('ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_div_conteudo_publicacao', {
                placeholder: 'Conteúdo da publicação',
                title: 'Conteúdo da publicação',
            });
        } catch (ex) { console.log(ex); }
        //Adicionando o bõtão de adicionar publicação
        adicionarBotaoAdicionarPublicacao();
        //Adicionando o botão de editar em cada publicação
        adicionarBotoesEdicaoPublicacao();
        //Sortable das imagens da publicação
        criarOrdenacaoImagensEdicaoPublicacao();
    }
    function adicionarBotaoAdicionarPublicacao() {
        if ($('.btn_adicionar_publicacao').length > 0)
            return;
        var divBotao = $('<div>', {
            class: 'text-center btn_adicionar_publicacao mb-2'
        });
        var botao = $('<div>', {
            class: 'btn btn-outline-primary btn-sm',
            'onclick': 'showDialogPublicacao(this)'
        }).appendTo(divBotao);
        var iBotao = $('<i>', {
            class: 'fa fa-plus-circle me-1'
        }).appendTo(botao);
        var spanBotao = $('<span>', {
            text: 'Adicionar nova publicação'
        }).appendTo(botao);
        $('.listagem_publicacoes_b4').prepend(divBotao);
    }
    function showDialogPublicacao(element) {
        $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_hfIdPublicacaoEdicaoSite').val('');
        $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_hfIdPublicacaoEdicaoSite').text('');
        $('.loading_edicao_publicacao').show();
        $('.container_edicao_publicacao').hide();
        $('#dlgPublicacaoEdicaoSite').modal('show');
        $('#btnAdicionarNovaPublicacao').click();
    }
    function saveContentPublicacao(element) {
        var tituloPreenchido = $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_txtTituloPublicacaoEdicaoSite').val();
        if (!tituloPreenchido || tituloPreenchido == null || tituloPreenchido == undefined || $.trim(tituloPreenchido) == '') {
            $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_txtTituloPublicacaoEdicaoSite').focus();
            return false;
        }
        $(element).attr('disabled', 'disabled').find('span').text('Aguarde...');
        var content = RemoveClassCKeditor('ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_div_conteudo_publicacao');
        $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_hfContentPublicacaoedicaoSite').text(content);
        $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_hfContentPublicacaoedicaoSite').val(content);
    }
    function adicionarBotoesEdicaoPublicacao() {
        $('.container_informativo_index_corpo .col-listagem').each(function () {
            if ($(this).find('.opcoes_edicao_publicacao').length < 1) {
                var divOpcoes = $('<div>', {
                    class: 'opcoes_edicao_publicacao d-flex justify-content-end'
                });
                var divOpcaoEdit = $('<div>', {
                    class: 'btn btn-warning rounded btn-sm px-3 py-1 text-center me-2',
                    title: 'Editar esta publicação',
                    'onclick': 'editarPublicacaoEdicaoSite(this)'
                }).appendTo(divOpcoes);;
                var iOpcaoEdit = $('<i>', {
                    class: 'fa fa-edit'
                }).appendTo(divOpcaoEdit);

                var divOpcaoExclusao = $('<div>', {
                    class: 'btn btn-danger rounded btn-sm px-3 py-1 text-center me-2',
                    title: 'Editar esta publicação',
                    'onclick': 'excluirPublicacaoEdicaoSite(this)'
                }).appendTo(divOpcoes);;
                var iOpcaoExclusao = $('<i>', {
                    class: 'fa fa-trash-o'
                }).appendTo(divOpcaoExclusao);

                divOpcoes.appendTo($(this).find('.card'));
            }
        });
    }
    function editarPublicacaoEdicaoSite(element) {
        var idPublicacao = $(element).closest('.card').data('id');
        if (!idPublicacao || idPublicacao == null || idPublicacao == undefined) {
            alert('Não achamos a publicação que está tentando editar. Recarregue a página e tente novamente!');
            return;
        }
        $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_hfIdPublicacaoEdicaoSite').val(idPublicacao);
        $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_hfIdPublicacaoEdicaoSite').text(idPublicacao);
        $('.loading_edicao_publicacao').show();
        $('.container_edicao_publicacao').hide();
        $('#dlgPublicacaoEdicaoSite').modal('show');
        $('#btnAdicionarNovaPublicacao').click();
    }
    function excluirPublicacaoEdicaoSite(element) {
        if (!confirm('Deseja realmente excluir esta publicação?')) return false;
        $(element).find('i').removeClass().addClass('fa fa-asterisk fa-spin');
        var idImagem = $(element).closest('.card').data('id');
        $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_hfIdPublicacaoEdicaoSite').val(idImagem);
        $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_hfIdPublicacaoEdicaoSite').text(idImagem);
        $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_btnExcluirPublicacaoListagemPublicacoesEdicaoSite').click();
    }
    function uploadFilesEdicaoPublicacao(element) {
        if ($(element)[0].files.length < 1) {
            toastWarning('', 'Nenhum arquivo selecionado!');
            return;
        }

        $('.loading_upload_imagem_edicao_publicacao').removeClass('d-none').text('Aguarde o upload...');
        var formData = new FormData();
        var ajaxUrl = "../EditControls.ashx";
        var requestAux = $('#requestIA').val();
        var idPublicacao = $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_hfIdPublicacaoEdicaoSite').val();

        formData.append('upload_imagem_publicacao', true);
        formData.append('request', requestAux);
        formData.append('idPublicacao', idPublicacao);
        for (index = 0; index < $(element)[0].files.length; index++) {
            formData.append('file_' + index, $(element)[0].files[index]);
        }
        $.ajax({
            url: ajaxUrl,
            type: "POST",
            data: formData,
            contentType: false,
            processData: false
        }).done(function (response) {
            $(element).val(null);
        }).fail(function (response) {
            console.log(response);
            toastDanger('Ocorreu um erro', response.responseText);
        }).always(function () {
            $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_btnAtualizarFotosEdicaoPublicacao').click();
        });
    }
    function criarOrdenacaoImagensEdicaoPublicacao() {
        $(".imagens_edicao_publicacao_edicao_site").sortable({
            tolerance: "pointer",
            forcePlaceholderSize: true,
            stop: function (evt) {
                var indexes = '';
                $('.foto_publicacao_edicao_site').each(function (index) {
                    indexes += $(this).data('id') + ';';
                });
                $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_hfPrioridadeImagensEdicaoPublicacao').val(indexes);
                $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_btnSalvarPrioridadeImagensEdicaoPublicacao').click();
            },
        }).disableSelection();
    }
    function fecharEAtualizarListagemPublicacoes() {
        $('.atualizar_listagem_publicacoes').click();
        $('#dlgPublicacaoEdicaoSite').modal('hide');
    }
    function toggleOpcoesImagensSelecionadas(element) {
        $(element).toggleClass('active');
        $('.opcoes_fotos_selecionadas').addClass('d-none');
        //Exibir opções das fotos selecionadas
        if ($('.foto_publicacao_edicao_site.active').length > 0) {
            $('.opcoes_fotos_selecionadas').removeClass('d-none');
        }
    }
    function adicionarFotosAoConteudoEdicaoPublicacao(element) {
        if ($('.foto_publicacao_edicao_site.active').length < 1) {
            toastWarning('Nenhuma imagem selecionada.');
            return;
        }
        var htmlFotos = '';
        $('.foto_publicacao_edicao_site.active').each(function () {
            htmlFotos += $(this).html();
        });
        CKEDITOR.instances.ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_div_conteudo_publicacao.insertHtml(htmlFotos);
    }
    function excluirFotosSelecionadasEdicaoPublicacao(element) {
        if (!confirm('Deseja realmente excluir as fotos selecionadas?'))
            return false;
        $(element).attr('disabled', 'disabled').unbind().find('i').removeClass().addClass('fa fa-asterisk fa-spin me-1');
        var ids = '';
        $('.foto_publicacao_edicao_site.active').each(function () {
            ids += ($(this).data('id') + ';');
        });
        $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_hfIdsFotosSelecionadasExclusaoEdicaoPublicacao').val(ids);
        $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_hfIdsFotosSelecionadasExclusaoEdicaoPublicacao').text(ids);
        $('#ctl00_ControlEdicao_ControlEdicaoListagemPublicacoes_btnExcluirFotosSelecionadasEdicaoPublicacao').click();
    }
</script>


        

